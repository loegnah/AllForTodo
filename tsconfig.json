{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "pretty": true,
    "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "paths": {
      "assets/*": ["assets/*"],
      "navigation/*": ["src/navigation/*"],
      "components/*": ["src/components/*"],
      "hooks/*": ["src/hooks/*"],
      "constants/*": ["src/constants/*"],
      "screens/*": ["src/screens/*"],
      "src/*": ["src/*"]
    }
  }
}
